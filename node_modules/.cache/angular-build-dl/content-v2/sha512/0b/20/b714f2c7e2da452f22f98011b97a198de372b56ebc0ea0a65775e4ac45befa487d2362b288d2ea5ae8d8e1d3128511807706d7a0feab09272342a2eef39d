    constructor() {
        this.data = [
            { id: 0, name: 'Hookah with 2 pipes', price: 149.99, amount: 0 },
            { id: 1, name: 'Charcoal (10)', price: 24.99, amount: 0 },
            { id: 2, name: 'Hookah Pipes *2', price: 79.99, amount: 0 },
            { id: 3, name: 'Shisha (any flavor)', price: 29.99, amount: 0 },
            { id: 4, name: 'Foils (20s)', price: 14.99, amount: 0 }
        ];
        this.cart = [];
        this.cartItemCount = new rxjs__WEBPACK_IMPORTED_MODULE_2__["BehaviorSubject"](0);
    }
    getProducts() {
        return this.data;
    }
    getCart() {
        return this.cart;
    }
    getCartItemCount() {
        return this.cartItemCount;
    }
    addProduct(product) {
        let added = false;
        for (let p of this.cart) {
            if (p.id === product.id) {
                p.amount += 1;
                added = true;
                break;
            }
        }
        if (!added) {
            product.amount = 1;
            this.cart.push(product);
        }
        this.cartItemCount.next(this.cartItemCount.value + 1);
    }
    decreaseProduct(product) {
        for (let [index, p] of this.cart.entries()) {
            if (p.id === product.id) {
                p.amount -= 1;
                if (p.amount == 0) {
                    this.cart.splice(index, 1);
                }
            }
        }
        this.cartItemCount.next(this.cartItemCount.value - 1);
    }
    removeProduct(product) {
        for (let [index, p] of this.cart.entries()) {
            if (p.id === product.id) {
                this.cartItemCount.next(this.cartItemCount.value - p.amount);
                this.cart.splice(index, 1);
            }
        }
    }
};
CartService.ctorParameters = () => [];
