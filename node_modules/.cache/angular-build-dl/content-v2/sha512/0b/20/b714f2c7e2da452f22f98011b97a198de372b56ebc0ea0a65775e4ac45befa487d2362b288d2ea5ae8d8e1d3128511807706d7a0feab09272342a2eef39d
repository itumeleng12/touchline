        return new Promise((resolve, reject) => {
            if (this.afAuth.currentUser) {
                this.afAuth.signOut()
                    .then(() => {
                    console.log("LOG Out");
                    resolve();
                }).catch((error) => {
                    reject();
                });
            }
        });
    }
